accentColor = #FFA100

h1
  color accentColor

body
  background #eee

#wrapper
  width 338px
  margin 80px auto

.col
  width 338px
  display inline-block

.col p
  color white
  background #FFF
  border solid 1px #333
  width 100px
  height 100px
  margin 0 0 8px 8px
  float left
  border-radius 8px

a
  color accentColor
  transition color .1s ease-in-out
